{
  "manifest_version": 1,
  "name": "海龟汤游戏插件 - turtlesoup_plugin",
  "display_name": "海龟汤游戏插件",
  "version": "1.6.0",
  "description": "一个交互式海龟汤（推理谜题）游戏插件，支持用户创建谜题、请求提示、提问并获得回答。目前支持0.10.1的mmc",
  "author": {
    "name": "Unreal and 何夕",
    "url": "https://github.com/Heximiao/turtlesoup_plugin"
  },
  "license": "MIT",
  "host_application": {
    "min_version": "0.10.1",
    "max_version": "0.10.1"
  },
  "entry": "plugin.py",
  "homepage_url": "https://github.com/Heximiao/turtlesoup_plugin",
  "repository_url": "https://github.com/Heximiao/turtlesoup_plugin",
  "keywords": ["游戏", "谜题", "海龟汤", "推理", "互动"],
  "categories": ["娱乐", "游戏"],
  "default_locale": "zh-CN",
  "locales_path": "_locales",
  "plugin_info": {
    "is_built_in": false,
    "plugin_type": "general",
    "components": [
      {
        "type": "command",
        "name": "hgt_create",
        "description": "创建一个新的海龟汤谜题"
      },
      {
        "type": "command",
        "name": "hgt_hint",
        "description": "请求一个提示"
      },
      {
        "type": "command",
        "name": "hgt_clue",
        "description": "请求整理线索"
      },
      {
        "type": "command",
        "name": "hgt_question",
        "description": "向谜题提问，获得是/否/无关的回答"
      },
      {
        "type": "command",
        "name": "hgt_answer",
        "description": "揭晓当前海龟汤谜题的答案"
      },
      {
        "type": "command",
        "name": "hgt_status",
        "description": "查看当前谜题状态"
      }
    ],
    "features": ["交互式谜题", "海龟汤游戏", "推理解谜"]
  }
}
